processes:
  - name: garden
    <%# executable: /var/vcap/jobs/garden/bin/garden_start %>
    executable: /bin/sleep
    args:
      - 99999
    env: {}
    ephemeral_disk: true
    additional_volumes:
    - path: /var/vcap/data/garden
      writable: true
    - path: /var/vcap/data/tmp
      writable: true
    - path: /var/vcap/data/grootfs/store/privileged
      writable: true
    - path: /var/vcap/data/grootfs/store/unprivileged
      writable: true
    hooks:
      pre_start: /var/vcap/jobs/garden/bin/garden_pre_start
